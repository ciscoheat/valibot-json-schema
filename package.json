{
  "name": "@gcornut/valibot-json-schema",
  "version": "0.0.6",
  "packageManager": "yarn@3.6.4",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gcornut/valibot-json-schema.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "valibot",
    "json-schema"
  ],
  "type": "module",
  "bin": "bin/valibot-json-schema.cjs",
  "files": ["dist", "bin"],
  "exports": {
    "./toJSONSchema": "./dist/toJSONSchema.js",
    "./extension/withJSONSchemaFeatures": "./dist/extension/withJSONSchemaFeatures.js"
  },
  "sideEffects": false,
  "scripts": {
    "build:lib": "rm -rf dist && mkdir dist && esbuild --bundle --format=esm src/toJSONSchema.ts src/extension/withJSONSchemaFeatures.ts --outdir=dist",
    "build:cli": "rm -rf bin && mkdir bin && esbuild --platform=node --target=node16 --bundle --external:esbuild-runner ./src/cli --outfile=bin/valibot-json-schema.cjs --banner:js=\"#!/usr/bin/env node\" && chmod +x bin/valibot-json-schema.cjs",
    "build": "yarn build:cli && yarn build:lib",
    "check:types": "tsc --noEmit",
    "publish": "yarn build && yarn npm publish",
    "run:cli": "esr src/cli",
    "test": "vitest"
  },
  "dependencies": {
    "esbuild-runner": "^2.2.2"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.2.1",
    "@types/json-schema": "^7.0.13",
    "@types/lodash": "^4.14.199",
    "@vitest/coverage-v8": "^0.34.6",
    "ajv": "^8.12.0",
    "commander": "^11.1.0",
    "esbuild": "^0.18.20",
    "lodash": "^4.17.21",
    "safe-stable-stringify": "^2.4.3",
    "ts-node": "^10.9.1",
    "typescript": "5.1.3",
    "valibot": "^0.19.0",
    "vitest": "0.32.2"
  }
}
